{
  "name": "craftcms/demo-spoke-and-chain",
  "description": "Spoke & Chain Demo",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-commerce",
    "commerce",
    "project",
    "demo",
    "starter"
  ],
  "license": "0BSD",
  "homepage": "https://craftcms.com/demo",
  "type": "project",
  "support": {
    "email": "support@craftcms.com"
  },
  "require": {
    "php": "^8.2",
    "bref/bref": "^2.0",
    "craftcms/aws-s3": "v3.x-dev",
    "craftcms/cms": "^5.0.0-beta.4",
    "craftcms/commerce": "^5.0.0-beta.1",
    "craftcms/redactor": "4.0.x-dev",
    "craftcms/store-hours": "4.0.x-dev",
    "fakerphp/faker": "^1.9",
    "nystudio107/craft-seomatic": "v5.x-dev",
    "nystudio107/craft-twigpack": "v5.x-dev",
    "solspace/craft-freeform": "dev-v5-craft5",
    "solspace/craft3-commons": "dev-dev-craft5 as 2.0.3",
    "oberon/starfield": "dev-master",
    "putyourlightson/craft-sprig": "v3.x-dev",
    "vlucas/phpdotenv": "^5.4.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/AugustMiller/craft-twigpack"
    },
    {
      "type": "vcs",
      "url": "https://github.com/craftcms/craft-freeform"
    },
    {
      "type": "vcs",
      "url": "https://github.com/craftcms/craft3-commons"
    },
    {
      "type": "vcs",
      "url": "https://github.com/craftcms/craft-starfield"
    },
    {
      "type": "vcs",
      "url": "https://github.com/craftcms/store-hours"
    }
  ],
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules",
      "modules\\demos\\": "modules/demos/src/",
      "sprig\\components\\": "sprig/components/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php craft db/restore seed.sql > /dev/null 2>&1",
      "@php craft demos/seed > /dev/null 2>&1"
    ],
    "phpstan": "phpstan --memory-limit=1G",
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
