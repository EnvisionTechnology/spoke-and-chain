{
  "name": "craftcms/demo-spoke-and-chain",
  "description": "Spoke & Chain Demo",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-commerce",
    "commerce",
    "project",
    "demo",
    "starter"
  ],
  "license": "0BSD",
  "homepage": "https://craftcms.com/demo",
  "type": "project",
  "support": {
    "email": "support@craftcms.com"
  },
  "require": {
    "craftcms/aws-s3": "2.0.0-beta.1",
    "craftcms/cms": "^4.0.0-alpha.1",
    "craftcms/commerce": "4.0.x-dev",
    "craftcms/redactor": "3.0.x-dev",
    "craftcms/store-hours": "v3.x-dev",
    "fakerphp/faker": "^1.9",
    "nystudio107/craft-seomatic": "4.0.0-beta.4",
    "nystudio107/craft-twigpack": "4.0.0-beta.2",
    "oberon/starfield": "dev-craft-4",
    "putyourlightson/craft-sprig": "2.0.0-alpha.1",
    "vlucas/phpdotenv": "^3.4.0",
    "solspace/craft-freeform": "dev-craft4"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules",
      "modules\\demos\\": "modules/demos/src/",
      "sprig\\components\\": "sprig/components/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mattstein/craft-starfield.git"
    },
    {
      "type": "git",
      "url": "https://github.com/brandonkelly/craft3-freeform"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php craft db/restore seed.sql > /dev/null 2>&1",
      "@php craft demos/seed > /dev/null 2>&1"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
