<head>
  {# Standard Project Meta #}
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {# Prevent site from being indexed #}
  <meta name="robots" content="noindex,nofollow">
  {# /Standard Project Meta #}

{#  {% include '_includes/layout/favicons' ignore missing with {} only %}#}

  {# Project Styles #}
  {{ craft.twigpack.includeCssModule('styles.css', false) }}
  {# /Project Styles #}

  {# Project Scripts #}
  <script>
    var SITE_URL = '{{ siteUrl }}';
    var CSRF = '{{ craft.app.request.getCsrfToken() }}';

    var addressCountries = {{ craft.commerce.store.getStore().getCountriesList()|json_encode|raw }};
    var addressStates = {{ craft.commerce.store.getStore().getCountriesList()|map((val, key) => craft.app.getAddresses().getAddressFormatRepository().get(key).getAdministrativeAreaType() == null ? null : craft.app.addresses.getSubdivisionRepository().getAll([key])|map((av, aa) => aa))|json_encode|raw }};
  </script>
  {# /Project Scripts #}
</head>